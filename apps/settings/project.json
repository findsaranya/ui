{
  "projectType": "application",
  "root": "apps/settings",
  "sourceRoot": "apps/settings/src",
  "prefix": "tt-webapp",
  "i18n": {
    "sourceLocale": {
      "code": "en"
    },
    "locales": {
      "bn": "apps/settings/src/locale/messages.xlf",
      "zh-Hant": "apps/settings/src/locale/messages.xlf",
      "zh": "apps/settings/src/locale/messages.xlf",
      "et": "apps/settings/src/locale/messages.xlf",
      "fr": "apps/settings/src/locale/messages.xlf",
      "hi": "apps/settings/src/locale/messages.xlf",
      "it": "apps/settings/src/locale/messages.xlf",
      "ja": "apps/settings/src/locale/messages.xlf",
      "kn": "apps/settings/src/locale/messages.xlf",
      "lv": "apps/settings/src/locale/messages.xlf",
      "lt": "apps/settings/src/locale/messages.xlf",
      "ml": "apps/settings/src/locale/messages.xlf",
      "pl": "apps/settings/src/locale/messages.xlf",
      "pt": "apps/settings/src/locale/messages.xlf",
      "ro": "apps/settings/src/locale/messages.xlf",
      "es": "apps/settings/src/locale/messages.xlf",
      "sv": "apps/settings/src/locale/messages.xlf",
      "ta": "apps/settings/src/locale/messages.xlf",
      "te": "apps/settings/src/locale/messages.xlf",
      "tr": "apps/settings/src/locale/messages.xlf",
      "vi": "apps/settings/src/locale/messages.xlf"
    }
  },
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/settings",
        "index": "apps/settings/src/index.html",
        "main": "apps/settings/src/main.ts",
        "polyfills": "apps/settings/src/polyfills.ts",
        "tsConfig": "apps/settings/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/settings/src/favicon.ico",
          "apps/settings/src/assets"
        ],
        "styles": [
          "libs/ui/src/styles.scss",
          "apps/auth/src/styles.scss"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/settings/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/settings/src/environments/environment.ts",
              "with": "apps/settings/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/settings/webpack.prod.config.js"
          },
          "i18nMissingTranslation": "error",
          "localize": [
            "bn",
            "zh-Hant",
            "zh",
            "en",
            "et",
            "fr",
            "hi",
            "it",
            "ja",
            "kn",
            "lv",
            "lt",
            "ml",
            "pl",
            "pt",
            "ro",
            "es",
            "sv",
            "ta",
            "te",
            "tr",
            "vi"
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "settings:build:production"
        },
        "development": {
          "browserTarget": "settings:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4202,
        "publicHost": "http://localhost:4202"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "settings:build",
        "outputPath": "apps/settings/src/locale/"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/settings/src/**/*.ts",
          "apps/settings/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/settings"
      ],
      "options": {
        "jestConfig": "apps/settings/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "Brand settings",
    "Supplier settings"
  ]
}